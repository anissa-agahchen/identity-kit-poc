{
  "env": "development",
  "apiServer": {
    "url": "http://localhost:5000"
  },
  "baseUrl": "http://localhost:4251",
  "csbAudio": {
    "siteUrl": "https://csb-audio.pathfinder.gov.bc.ca"
  },
  "oidc": {
    "stsServer": "http://localhost:8180/auth/realms/identity-kit",
    "redirect_url": "http://localhost:4251",
    "client_id": "identity-kit-public",
    "scope": "openid profile email",
    "response_type": "code",
    "trigger_authorization_result_event": true,
    "post_logout_redirect_uri": "http://localhost:4251/unauthorized",
    "start_checksession": true,
    "silent_renew": false,
    "silent_renew_url": "http://localhost:4251/assets/silent-renew.html",
    "silent_renew_offset_in_seconds": 200,
    "post_login_route": "/accept",
    "forbidden_route": "/forbidden",
    "unauthorized_route": "/unauthorized",
    "log_console_warning_active": false,
    "log_console_debug_active": true,
    "max_id_token_iat_offset_allowed_in_seconds": 30,
    "history_cleanup_off": true
  }
}
